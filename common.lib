#!/bin/bash
#
# Library functions to include

########################
# Check if program is installed
# Arguments:
#  program_name string
# Returns:
#  1 if is installed, otherwise return 0
########################
check_if_installed() {
 var_program_name=$1
 which $var_program_name &> /dev/null
 if [[ $? -eq 0 ]]; then
  return 1
 else
  return 0
 fi
}
